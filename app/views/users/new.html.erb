<h2>Create an Account</h2>
<div class="form-signin">
<%= form_for @user do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<p></p>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<p></p>
<%= f.label :city %>
<%= f.text_field :city %>
<p></p>
<%= f.label :state %>
<%= f.text_field :state %>
<p></p>
<%= f.label :username %>
<%= f.text_field :username %>
<p></p>
<%= f.label :password %>
<%= f.password_field :password %>
<p></p>
<%= f.submit "Submit" %>
<% end %>
</div>
